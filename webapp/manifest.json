{
    "name": "USB Nico CNC",
    "version": "0.1",
    "manifest_version": 2,
    "minimum_chrome_version": "23",
    "permissions": ["usb", "storage", "webview",
        "https://auth.firebase.com/",
        {
            "usbDevices": [
                {
                    "vendorId": 1155,
                    "productId": 65535
                }
            ]
        }
    ],
    "app": {
        "background": {
            "scripts": ["background.js"]
        }
    },
    "sandbox": {
        "pages": ["js_sandbox.html"]
    },
    "icons": {
        "48": "images/icon_fraise_48.png",
        "128": "images/icon_fraise_128.png"
    }
}
